  task project "Project Targets/Metrics" {
    task reqs "Stakeholders/Requirements matrix" {
      purge allocate
      allocate all
      start 2019-09-11
      duration 7d
    }
    task rankreqs "Rank Requirements" {
      depends !reqs
      purge allocate
      allocate all
      duration 1d
    }
    task scope "Scope" {
      purge allocate
      allocate hamish
      start 2019-09-16
      duration 1d
    }
    task valueprop "Value Proposition" {
      purge allocate
      allocate hamish
      start 2019-09-16
      duration 1d
    }
  }
  task function "Function Decomposition" {
    task topfuncdecomp "Top Level Function Decomposition" {
      purge allocate
      allocate all
      start 2019-09-18
      duration 1d
    }
    task funcdecomp "Function Tree Diagram" {
      depends !topfuncdecomp
      purge allocate
      allocate all
      duration 4d
    }
  }
  task concept "Concept Generation" {
    task concgenfrag "Concept Fragment Generation" {
      depends !!function.funcdecomp
      purge allocate
      allocate all
      duration 5d
    }
    task concgen "Full Concept Generation" {
      depends !concgenfrag
      purge allocate
      allocate all
      duration 5d
    }
  }
  task concept_selection "Concept Selection" {
    task winnow "Winnowing" {
      depends !!concept.concgen
      purge allocate
      allocate all
      duration 3d
    }
    task rank "Ranking" {
      depends !winnow
      purge allocate
      allocate all
      duration 3d
    }
    task score "Scoring" {
      depends !rank
      purge allocate
      allocate all
      duration 3d
    }
  }
  task did "Design in Detail" {
    depends !concept_selection.score
    purge allocate
    allocate all
    duration 30d
  }
  task report "Final Report" {
    purge allocate
    allocate all
    duration 30d
    end 2019-11-16
  }
  task oral_pres "Oral Presentation" {
    duration 14d
    end 2019-11-18
  }
  task milestones "Milestones" {
    task weekly "Weekly Reports" {
      task rep1 "Report 1" {
        milestone
        start 2019-09-11
      }
      task rep2 "Report 2" {
        milestone
        start 2019-09-18
      }
      task rep3 "Report 3" {
