        start 2019-10-09
      }
      task rep6 "Report 6" {
        milestone
        start 2019-10-16
      }
      task rep7 "Report 7" {
        milestone
        start 2019-10-23
      }
      task rep8 "Report 8" {
        milestone
        start 2019-10-30
      }
      task rep9 "Report 9" {
        milestone
        start 2019-11-06
      }
      task rep10 "Report 10" {
        milestone
        start 2019-11-13
      }
    }
    task csr "Concept Selection Review" {
      milestone
      start 2019-10-09
    }
    task final_report_due "Final Report" {
      milestone
      start 2019-11-16
    }
    task oral_pres_due "Oral Presentation" {
      milestone
      start 2019-11-18
    }
  }
}
textreport report "Plan" {
  formats html
  header '== TrailRider Project =='

  center -8<-
    [#Plan Plan] | [#Resource_Allocation Resource Allocation]
    ----
    === Plan ===
    <[report id="plan"]>
    ----
    === Resource Allocation ===
    <[report id="resourceGraph"]>
  ->8-
}

# A traditional Gantt chart with a project overview.
taskreport plan "" {
  headline "Project Plan"
  columns bsi, name, start, end, effort, chart { scale day width 1000 }
  loadunit shortauto
  hideresource 1
}

# A graph showing resource allocation. It identifies whether each
# resource is under- or over-allocated for.
resourcereport resourceGraph "" {
  headline "Resource Allocation Graph"
  columns no, name, effort, weekly
  loadunit shortauto
  hidetask ~(isleaf() & isleaf_())
  sorttasks plan.start.up
}
