task trailrider "TrailRider Project" {
  start 2019-09-06
  end 2019-11-29
  task research "Research" {
    purge allocate
    allocate all
    start 2019-09-06
    duration 14d
  }
  task gantt "Gantt Chart" {
    purge allocate
    allocate jasper
    start 2019-09-10
    duration 1d
